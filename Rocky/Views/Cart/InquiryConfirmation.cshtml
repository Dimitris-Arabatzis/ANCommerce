@model Rocky_Models.OrderHeader

<div class="text-center">

    @if (Model != null)
    {
        <h1 class="text-info">Order Created</h1>
        <p>Order #@Model.Id has been created successfully!</p>
    }
    else
    {
        <h1 class="text-info">Inquiry Information</h1>
        <p>We have received your enquiry and someone from our team will contact you shortly!</p>
    }

    <img src="/images/patio.jpg" width="80%" />
</div>